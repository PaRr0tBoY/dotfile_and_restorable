				<string>Getting Bundle ID of apps in /Applicationsâ€¦</string>
				<key>script</key>
				<string>require 'json'
require 'open3'
require 'pathname'

apps = Pathname('/Applications').children
         .concat(Pathname('/Applications/Utilities').children)
         .map(&amp;:to_path)
         .sort_by { |p| File.basename(p).downcase }
         .select { |e| e.end_with?('.app') }

script_filter_items = []

apps.each do |app|
  appId = Open3.capture2("osascript -e 'id of app \"#{app}\"'").first
  next if appId == '(null)'
  name = File.basename(app, '.app')
  script_filter_items.push(title: name, subtitle: appId, arg: appId, type:'file:skipcheck',icon: { type: 'fileicon', path: app },action:{ file:app})
end

puts({ items: script_filter_items }.to_json)</string>
