				<string>#!/bin/zsh

readonly data="${alfred_workflow_data}"
readonly file="$data/list.txt"
[[ -d $data ]] || mkdir -p $data
[[ -f $file ]] || touch -r $file
readonly favs=("${(f)$(cat $file)}")
readonly paths=("$@")

printf "\n" &gt;&gt; $file

typeset -i addc
local addc=0
for path in "${paths[@]}"; do
	## add iff not in list
	if [[ "${favs[(ie)$path]}" -gt ${#favs} ]]; then
		addc+=1
		echo "$path" &gt;&gt; $file 
	fi
done

printf "\n" &gt;&gt; $file

if [[ $addc -eq 0 &amp;&amp; ${#paths[@]} -eq 1 ]]; then
	print "Favorite '${paths[1]:t:r}' is already on the List"
elif [[ ${#paths[@]} -eq 1 ]]; then
	print "Added '${paths[1]:t:r}' to Favorites"
else 
	print "Added $addc items to Favorites"
fi</string>
