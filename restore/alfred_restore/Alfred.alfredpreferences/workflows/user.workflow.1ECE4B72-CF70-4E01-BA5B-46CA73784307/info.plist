				<string>#!/bin/zsh

readonly data="${alfred_workflow_data}"
readonly file="$data/list.txt"
[[ -d $data ]] || mkdir -p $data
[[ -f $file ]] || touch -r $file
[[ $1 == ":fav" ]] &amp;&amp; { open $file ; exit 0 }

sed -i '' '/./!d' $file
local favs=("${(f)$(cat $file)}")

if [[ -n $1 ]]; then
    set -A filtered
    for fav in "${favs[@]}"; do
		local n="$fav:t"
		[[ -f $fav ]] || n=$fav:t
		if echo "$n" | tr -d ' ' | grep -qi "$1"; then
			filtered+=("$fav")
		fi
    done
    favs=($filtered)
fi

if [[ -z $favs ]]; then
    echo -n "{\"items\":[{\"title\":\"Add some Favorites!\"}]}"
else 
    items="{\"skipknowledge\":true,\"items\":["
    for ((i = 1; i &lt;= ${#favs[@]}; i += 1 )); do
		local p=$favs[$i] # fav path
		local n=$p:t:r    # displayname
		local icon="\"icon\":{\"type\":\"fileicon\",\"path\":\"$p\"}"
		local vars="\"variables\":{\"line\":\"$i\"}"

		[[ -z $1   ]] || vars="\"variables\":{\"line\":\"$i\",\"nomove\":\"1\"}"
		[[ -f "$p" ]] || n=$p:t # dirname
		[[ "$p:t:e" =~ "[pP][dD][fF]" ]] &amp;&amp; icon="\"icon\":{\"path\":\"pdf.png\"}"

		items+="{\"title\":\"$n\",\"arg\":\"$p\",\"uid\":\"$n\","$vars",\"type\":\"file:skipcheck\",$icon,\"mods\":{\"ctrl\":{\"subtitle\":\"${p/$HOME/~}\",\"valid\":false}}},"
    done
    items=${items[1,-2]}
    items+="]}"
    echo -n $items
fi</string>
