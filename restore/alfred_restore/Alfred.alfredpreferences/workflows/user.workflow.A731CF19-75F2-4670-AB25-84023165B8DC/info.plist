				<key>script</key>
				<string>readonly cache="$alfred_workflow_cache"
readonly log_file="${cache}/pull_progress.txt"
readonly info_file="${cache}/pull_info.txt"
readonly canceled_file="${cache}/pull_canceled.txt"

echo -n "$1" &gt; "$info_file"

# ollama pull "$1" &amp;&gt; &gt;(tee "$log_file" &gt; /dev/null) &amp;
ollama pull "$1" &amp;&gt; "$log_file" &amp; 

# # Not the PID of the actual pull process.
# # We cannot use it to cancel the download
# # but to confirm that it is still running.
pid=$!

while ps -p $pid &gt; /dev/null; do
	sleep 1
done

# # End the process if it is still lingering.
if ps -p $pid &gt; /dev/null; then
	# kill $pid
	killall ollama
fi

/bin/rm $log_file
/bin/rm $info_file

if [[ -f $canceled_file ]]; then
	/bin/rm $canceled_file
	echo -n "Download Canceled"

else
	echo -n "Download Complete"
fi</string>
